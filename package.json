{
  "name": "wappsto-cli",
  "version": "1.5.5",
  "description": "Command Line Interface for Wappsto",
  "scripts": {
    "lint": "eslint --ignore-pattern examples --ignore-path .gitignore  .",
    "lint:fix": "yarn lint --fix",
    "pretest": "yarn lint:fix",
    "test": "nyc ava -sv test/*.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepare": "husky install"
  },
  "type": "commonjs",
  "engines": {
    "node": ">=16.4.0"
  },
  "bin": {
    "wapp": "./bin/wapp.js",
    "create-wapp": "./bin/create.js",
    "update-wapp": "./bin/update.js",
    "delete-wapp": "./bin/delete.js",
    "serve-wapp": "./bin/serve.js",
    "configure-wapp": "./bin/configure.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wappsto/wappsto-cli.git"
  },
  "keywords": [
    "wappsto",
    "wapp",
    "api",
    "iot",
    "seluxit"
  ],
  "author": {
    "name": "Seluxit A/S",
    "email": "developer@wappsto.com",
    "url": "https://wappsto.com"
  },
  "files": [
    "bin/*.js",
    "lib/*.js",
    "examples/*"
  ],
  "directories": {
    "bin": "./bin",
    "lib": "./lib",
    "example": "./examples",
    "test": "test"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wappsto/wappsto-cli/issues"
  },
  "homepage": "https://github.com/wappsto/wappsto-cli#readme",
  "dependencies": {
    "async-mutex": "^0.3.1",
    "axios": "^0.26.1",
    "browser-sync": "2.27.9",
    "chalk": "^4.1.2",
    "command-line-args": "^5.2.1",
    "command-line-usage": "^6.1.0",
    "cross-spawn": "^7.0.3",
    "detect-port": "^1.3.0",
    "figlet": "^1.5.0",
    "form-data": "^4.0.0",
    "http-proxy-middleware": "2.0.4",
    "inquirer": "^8.2.1",
    "node-watch": "^0.7.3",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@ava/babel": "^2.0.0",
    "ava": "^3.15.0",
    "axios-mock-adapter": "^1.19.0",
    "coveralls": "^3.1.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-import": "^2.23.2",
    "esm": "^3.2.25",
    "husky": "^7.0.4",
    "mock-inquirer": "^1.0.3",
    "mock-require": "^3.0.3",
    "nyc": "^15.1.0",
    "sinon": "^13.0.1"
  },
  "ava": {
    "babel": true,
    "require": [
      "esm",
      "./test/mock/backend.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  }
}
