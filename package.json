    },
    "author": {
        "name": "Seluxit A/S",
        "email": "developer@wappsto.com",
        "url": "https://wappsto.com"
    },
    "files": [
        "bin/*",
        "lib/*",
        "examples/*"
    ],
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/wappsto/wappsto-cli/issues"
    },
    "homepage": "https://github.com/wappsto/wappsto-cli#readme",
    "dependencies": {
        "axios": "^0.18.0",
        "chalk": "^2.4.2",
        "clui": "^0.3.6",
        "express": "^4.16.4",
        "figlet": "^1.2.1",
        "form-data": "^2.3.3",
        "http-proxy-middleware": "^0.19.1",
        "inquirer": "^6.2.2",
        "mkdirp": "^0.5.1",
        "ws": "^6.2.0"
    },
    "devDependencies": {
        "ava": "^1.3.1",
