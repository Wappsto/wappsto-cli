{
    "name": "wappsto-cli",
    "version": "1.2.0",
    "description": "Command Line Interface for Wappsto",
    "scripts": {
        "pretest": "eslint --ignore-pattern examples --ignore-path .gitignore  .",
        "test": "nyc ava -sv test/*.js",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "release": "np"
    },
    "bin": {
        "create-wapp": "./bin/create.js",
        "update-wapp": "./bin/update.js",
        "delete-wapp": "./bin/delete.js",
        "serve-wapp": "./bin/serve.js"
    },
    "main": "lib/wapp.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wappsto/wappsto-cli.git"
    },
    "keywords": [
        "wappsto",
        "wapp",
        "api",
        "iot",
        "seluxit"
    ],
    "author": {
        "name": "Seluxit A/S",
        "email": "developer@wappsto.com",
        "url": "https://wappsto.com"
    },
    "files": [
        "bin/*.js",
        "lib/*.js",
        "examples/*"
    ],
    "directories": {
        "bin": "./bin",
        "lib": "./lib",
        "example": "./examples",
        "test": "test"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/wappsto/wappsto-cli/issues"
    },
    "homepage": "https://github.com/wappsto/wappsto-cli#readme",
    "dependencies": {
        "axios": "^0.19.0",
        "chalk": "^2.4.2",
        "figlet": "^1.2.4",
        "form-data": "^2.5.1",
        "http-proxy": "^1.18.0",
        "inquirer": "^6.5.1",
        "ws": "^6.2.1"
    },
    "devDependencies": {
        "ava": "^1.4.1",
        "axios-mock-adapter": "^1.17.0",
        "coveralls": "^3.0.6",
        "eslint": "^5.16.0",
        "eslint-config-airbnb-base": "^13.2.0",
        "eslint-plugin-ava": "^5.1.1",
        "eslint-plugin-import": "^2.21.2",
        "husky": "^1.3.1",
        "mock-inquirer": "^1.0.3",
        "mock-require": "^3.0.3",
        "np": "^4.0.2",
        "nyc": "^14.1.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm test",
            "pre-push": "npm test"
        }
    }
}
