        "coveralls": "^3.0.3",
        "eslint": "^5.15.1",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-ava": "^5.1.1",
        "eslint-plugin-import": "^2.16.0",
        "mock-inquirer": "^1.0.2",
        "mock-require": "^3.0.3",
        "np": "^4.0.2",
        "nyc": "^13.3.0"
