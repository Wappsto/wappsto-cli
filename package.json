{
    "name": "wappsto-cli",
    "version": "1.3.3",
    "description": "Command Line Interface for Wappsto",
    "scripts": {
        "pretest": "eslint --ignore-pattern examples --ignore-path .gitignore  . --fix",
        "test": "nyc ava -sv test/*.js",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "release": "np",
        "prepare": "husky install"
    },
    "bin": {
        "create-wapp": "./bin/create.js",
        "update-wapp": "./bin/update.js",
        "delete-wapp": "./bin/delete.js",
        "serve-wapp": "./bin/serve.js",
        "help": "./bin/help.js"
    },
    "main": "lib/wapp.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wappsto/wappsto-cli.git"
    },
    "keywords": [
        "wappsto",
        "wapp",
        "api",
        "iot",
        "seluxit"
    ],
    "author": {
        "name": "Seluxit A/S",
        "email": "developer@wappsto.com",
        "url": "https://wappsto.com"
    },
    "files": [
        "bin/*.js",
        "lib/*.js",
        "examples/*"
    ],
    "directories": {
        "bin": "./bin",
        "lib": "./lib",
        "example": "./examples",
        "test": "test"
    },
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/wappsto/wappsto-cli/issues"
    },
    "homepage": "https://github.com/wappsto/wappsto-cli#readme",
    "dependencies": {
        "axios": "^0.21.1",
        "chalk": "^4.1.1",
        "command-line-args": "^5.1.1",
        "command-line-usage": "^6.1.0",
        "figlet": "^1.5.0",
        "form-data": "^4.0.0",
        "http-proxy": "^1.18.1",
        "inquirer": "^8.1.0",
        "node-watch": "^0.7.1",
        "ws": "^7.4.6"
    },
    "devDependencies": {
        "ava": "^3.15.0",
        "axios-mock-adapter": "^1.19.0",
        "coveralls": "^3.1.0",
        "eslint": "^7.28.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-ava": "^12.0.0",
        "eslint-plugin-import": "^2.23.2",
        "esm": "^3.2.25",
        "husky": "^6.0.0",
        "mock-inquirer": "^1.0.3",
        "mock-require": "^3.0.3",
        "np": "^7.5.0",
        "nyc": "^15.1.0"
    },
    "ava": {
        "require": [
            "esm",
            "./test/mock/backend.js"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm test",
            "pre-push": "npm test"
        }
    }
}
