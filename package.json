{
  "name": "wappsto-cli",
  "description": "Command Line Interface for Wappsto",
  "version": "2.0.0",
  "license": "Apache-2.0",
  "main": "dist/wappsto-cli.esm.js",
  "module": "dist/wappsto-cli.esm.js",
  "type": "module",
  "author": {
    "name": "Seluxit A/S",
    "email": "developer@wappsto.com",
    "url": "https://seluxit.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wappsto/wappsto-cli.git"
  },
  "bugs": {
    "url": "https://github.com/wappsto/wappsto-cli/issues"
  },
  "homepage": "https://github.com/wappsto/wappsto-cli#readme",
  "keywords": [
    "wappsto",
    "wapp",
    "api",
    "iot",
    "seluxit"
  ],
  "files": [
    "dist"
  ],
  "bin": {
    "wapp": "./dist/wappsto-cli.esm.js"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint src test",
    "lint:fix": "yarn lint --fix",
    "prettify": "yarn run prettier -w src/ test/",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  },
  "engines": {
    "node": ">=16"
  },
  "size-limit": [
    {
      "path": "dist/wappsto-cli.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/wappsto-cli.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/axios": "^0.14.0",
    "@types/browser-sync": "^2.26.3",
    "@types/command-line-args": "^5.2.0",
    "@types/command-line-usage": "^5.0.2",
    "@types/cross-spawn": "^6.0.2",
    "@types/detect-port": "^1.3.2",
    "@types/figlet": "^1.5.5",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/prompts": "^2.4.2",
    "@types/ws": "^8.5.4",
    "axios-mock-adapter": "^1.21.2",
    "dts-cli": "^1.6.3",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "size-limit": "^8.1.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "async-mutex": "^0.4.0",
    "axios": "^1.2.2",
    "browser-sync": "^2.27.11",
    "command-line-args": "^5.2.1",
    "command-line-usage": "^6.1.3",
    "cross-spawn": "^7.0.3",
    "detect-port": "^1.5.1",
    "figlet": "^1.5.2",
    "form-data": "^4.0.0",
    "http-proxy-middleware": "^2.0.6",
    "kleur": "^4.1.5",
    "node-watch": "^0.7.3",
    "prompts": "^2.4.2",
    "simple-update-notifier": "^1.1.0",
    "ws": "^8.12.0"
  }
}
