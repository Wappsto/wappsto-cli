{
  "name": "wappsto-cli",
  "description": "Command Line Interface for Wappsto",
  "version": "1.5.4",
  "author": {
    "name": "Seluxit A/S",
    "email": "developer@wappsto.com",
    "url": "https://wappsto.com"
  },
  "type": "module",
  "scripts": {
    "lint": "eslint --ignore-pattern examples --ignore-path .gitignore  .",
    "lint:fix": "yarn lint --fix",
    "posttest": "yarn lint:fix",
    "test": "jest --coverage --runInBand",
    "prettify": "prettier --write .",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16.4.0"
  },
  "bin": {
    "wapp": "./bin/wapp.js",
    "create-wapp": "./bin/create.js",
    "update-wapp": "./bin/update.js",
    "delete-wapp": "./bin/delete.js",
    "serve-wapp": "./bin/serve.js",
    "configure-wapp": "./bin/configure.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wappsto/wappsto-cli.git"
  },
  "keywords": [
    "wappsto",
    "wapp",
    "api",
    "iot",
    "seluxit"
  ],
  "files": [
    "bin/*.js",
    "lib/*.js",
    "examples/*"
  ],
  "directories": {
    "bin": "./bin",
    "lib": "./lib",
    "example": "./examples",
    "test": "test"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wappsto/wappsto-cli/issues"
  },
  "homepage": "https://github.com/wappsto/wappsto-cli#readme",
  "dependencies": {
    "async-mutex": "^0.3.1",
    "axios": "^0.26.1",
    "browser-sync": "2.27.9",
    "chalk": "^4.1.2",
    "command-line-args": "^5.2.1",
    "command-line-usage": "^6.1.0",
    "detect-port": "^1.3.0",
    "figlet": "^1.5.0",
    "form-data": "^4.0.0",
    "http-proxy-middleware": "2.0.4",
    "inquirer": "^8.2.1",
    "node-watch": "^0.7.3",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "babel-jest": "^27.5.1",
    "eslint": "^8.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "esm": "^3.2.25",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-mock-axios": "^4.5.0",
    "jest-websocket-mock": "^2.3.0",
    "mock-inquirer": "^1.0.3",
    "prettier": "2.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test",
      "pre-push": "npm test"
    }
  }
}
